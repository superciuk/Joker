<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.joker.jokerapp.entity.ProductItemCategory"
          extends="_local"
          name="productCategory-view"/>
    <view class="com.joker.jokerapp.entity.ProductItem"
          extends="_local"
          name="productItem-view">
        <property name="category"
                  view="productCategory-view"/>
        <property name="printerGroup"
                  view="_minimal">
            <property name="printer"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.joker.jokerapp.entity.Printer"
          extends="_local"
          name="printer-view"/>
    <view class="com.joker.jokerapp.entity.PrinterGroup"
          extends="_local"
          name="printerGroup-view">
        <property name="printer"
                  view="_minimal"/>
    </view>
</views>
