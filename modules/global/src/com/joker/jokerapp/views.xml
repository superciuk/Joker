          extends="_local"
          name="productCategory-view"/>
    <view class="com.joker.jokerapp.entity.ProductItem"
          extends="_local"
          name="productItem-view">
        <property name="category"
                  view="_minimal"/>
        <property name="productItemModifierCategoryAssocs"
                  view="_local">
            <property name="productModifierCategory"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.joker.jokerapp.entity.ProductModifierCategory"
          extends="_local"
          name="productModifierCategory-view"/>
    <view class="com.joker.jokerapp.entity.ProductModifier"
          extends="_local"
          name="productModifier-view">
        <property name="category"
                  view="_minimal"/>
    </view>
    <view class="com.joker.jokerapp.entity.ProductItemModifierCategoryAssoc"
          extends="_local"
          name="productItemModifierCategoryAssoc-view">
        <property name="productModifierCategory"
                  view="_minimal"/>
    </view>
    <view class="com.joker.jokerapp.entity.TableItem"
          extends="_local"
          name="tableItem-view"/>
