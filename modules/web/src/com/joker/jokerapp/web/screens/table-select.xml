<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.joker.jokerapp.web.screens.TableSelect"
        messagesPack="com.joker.jokerapp.web.screens">
    <dsContext>
        <groupDatasource id="tableItemsDs"
                         class="com.joker.jokerapp.entity.TableItem"
                         view="tableItem-view">
            <query>
                <![CDATA[select e from jokerapp$TableItem e order by e.number]]>
            </query>
        </groupDatasource>
        <datasource id="tableItemDs"
                    class="com.joker.jokerapp.entity.TableItem"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout margin="true">
        <grid id="grid"
              margin="true"
              spacing="true">
            <columns count="1"/>
            <rows>
                <row/>
            </rows>
        </grid>
    </layout>
</window>
